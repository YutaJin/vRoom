<div class='main-contents'>
  <div class='col-sm-12'>
  <div class='group-post'>
    <%= render partial: "albummenu" %>
 <% @gposts.each do |post| %>
        <div class='post'>
          <div class='user-info'>

             <div class="image"  style="background-image: url(<%= rails_blob_path(post.user.image) %>) "></div>
            <p><%= post.user.nickname %></p>
          </div>
          <div class='more-content'>
          <div class="post-image" style="background-image: url(<%= rails_blob_path(post.image) %>)">
            <div class='post-sentence'>
            <%= post.text %>
          </div>
           <div class='post-menu'>
             <div class="dropdown">
              <li class='dropdown'>
                <a href='#' class='dropdown-toggle' data-toggle='dropdown' role='button' aria-expanded='false'><span class='caret'></span></a>
                 <ul class='dropdown-menu' role='menu'>
                    <li class='presentation'>
                      <%= link_to '編集', edit_group_g_album_gpost_path(@group, @g_album, post) %>
                  </li>
                  <li class='presentation'>
                   <%= link_to '削除', "/groups/#{@group.id}/g_albums/#{@g_album.id}/gposts/#{post.id}", method: :delete  %>
                  </li>
                   <li class='presentation'>

                  </li>
                </ul>
              </li>
            </div>
          </div>
          </div>
        </div>
    </div>
    <% end %>
  </div>

</div>
</div>


</div>
</div>
</div>
