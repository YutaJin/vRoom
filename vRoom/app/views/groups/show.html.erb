<div class='main-contents'>
  <div class='col-sm-12'>
  <div class='group-post'>
 <%= render partial:"groupmenu" %>
<h1>albums</h1>
<div class='album-index'>
  <% @g_album.each do |album| %>
  <div class='album'>
  <a href="/groups/<%= album.group_id %>/g_albums/<%= album.id %>">
  <div class="image"  style="background-image: url(<%= rails_blob_path(album.image) %>) ">
    <%=  album.name %>
    </div>
  </a>
</div>
 <% end %>
</div>

</div>
</div>
</div>






