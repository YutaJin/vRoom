<div class='search'>
  <div class='col-lg-12'>
<p>ユーザー検索</p>
<%= form_tag(search_path,:method => 'get') do %>
  <%= text_field_tag :search %>
  <%= submit_tag 'Search', :name => nil %>
<% end %>

<ul><% @user.each do |user| %>

  <li class='ff'>
    <a href="/users/<%= user.id %>" >
    <div class='nickname' >

    <%= user.nickname%>
  </div>

  </a>
  </li>
  <% end %>
</ul>

</div>
</div>
